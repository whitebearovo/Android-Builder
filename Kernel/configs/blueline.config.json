[
  {
    "kernelSource": {
      "name": "Nekomiya",
      "repo": "https://github.com/ReallySnow/msm-4.9",
      "branch": "thirteen",
      "device": "blueline",
      "defconfig": "b1c1_defconfig"
    },
    "toolchains": [
      {
        "name": "clang",
        "url": "https://android.googlesource.com/platform/prebuilts/clang/host/linux-x86/+archive/refs/heads/master-kernel-build-2022/clang-r450784d.ta",
        "binaryEnv": ["./bin"]
      },
      {
        "name": "gcc",
        "url": "https://android.googlesource.com/platform/prebuilts/gcc/linux-x86/aarch64/aarch64-linux-android-4.9/+archive/refs/tags/android-12.1.0_r27.tar.gz",
        "binaryEnv": ["bin"]
      }
    ],
    "enableCcache": true,
    "params": {
      "ARCH": "arm64",
      "CC": "",
      "externalCommands": {
        "CROSS_COMPILE": "aarch64-linux-android-",
        "CROSS_COMPILE_ARM32": "arm-linux-androideabi-",
        "CLANG_TRIPLE": "aarch64-linux-gnu-"
      }
    },
    "AnyKernel3": {
      "use": true,
      "release": true,
      }
    },
    "enableKernelSU": true,
    "enableLXC": false
  }
]