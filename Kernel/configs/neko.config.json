[
    {
      "kernelSource": {
        "name": "Neko",
        "repo": "https://github.com/ReallySnow/msm-4.9",
        "branch": "thirteen",
        "device": "blueline",
        "defconfig": "b1c1_defconfig"
      },
      "toolchains": [
        {
          "name": "proton-clang",
          "repo": "https://github.com/kdrag0n/proton-clang",
          "branch": "master",
          "binaryEnv": ["./bin"]
        },
        {
          "name": "gcc",
          "url": "https://android.googlesource.com/platform/prebuilts/gcc/linux-x86/aarch64/aarch64-linux-android-4.9/+archive/refs/tags/android-12.1.0_r27.tar.gz",
          "binaryEnv": ["bin"]
        }
      ],
      "enableCcache": true,
      "params": {
        "ARCH": "arm64",
        "CC": "clang",
        "externalCommands": {
          "CROSS_COMPILE": "proton-clang/bin/aarch64-linux-gnu-",
          "CROSS_COMPILE_ARM32": "proton-clang/bin/arm-linux-gnueabi-",
          "CLANG_TRIPLE": "aarch64-linux-gnu-"
        }
      },
      "AnyKernel3": {
        "use": true,
        "release": true,
        "custom": {
          "repo": "https://github.com/easterNday/AnyKernel3/",
          "branch": "thyme"
        }
      },
      "enableKernelSU": true,
      "enableLXC": false
    }
  ]